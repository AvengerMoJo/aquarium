{
  "name": "glass",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4242",
    "build": "ng build",
    "test": "jest --watch",
    "test:ci": "jest",
    "lint:eslint": "ng lint",
    "lint:html": "htmllint src/app/**/*.html && html-linter --config html-linter.config.json",
    "lint": "run-p -csl --aggregate-output lint:*",
    "fix:prettier": "prettier --write \"{src,e2e}/**/*.{ts,scss}\"",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix": "run-p -csl --aggregate-output fix:*",
    "e2e": "ng e2e"
  },
  "private": true,
  "jest": {
    "moduleNameMapper": {
      "~/(.*)$": "<rootDir>/src/$1"
    },
    "preset": "jest-preset-angular",
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "testMatch": [
      "**/*.spec.ts"
    ]
  },
  "dependencies": {
    "@angular/animations": "11.2.0",
    "@angular/cdk": "11.2.0",
    "@angular/common": "11.2.0",
    "@angular/compiler": "11.2.0",
    "@angular/core": "11.2.0",
    "@angular/flex-layout": "11.0.0-beta.33",
    "@angular/forms": "11.2.0",
    "@angular/material": "11.2.0",
    "@angular/platform-browser": "11.2.0",
    "@angular/platform-browser-dynamic": "11.2.0",
    "@angular/router": "11.2.0",
    "@mdi/angular-material": "5.9.55",
    "@swimlane/ngx-charts": "^17.0.0",
    "lodash": "4.17.20",
    "ng-block-ui": "3.0.2",
    "rxjs": "6.6.0",
    "tslib": "2.0.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "11.0.0",
    "@angular-devkit/build-angular": "0.1101.2",
    "@angular-eslint/builder": "1.1.0",
    "@angular-eslint/eslint-plugin": "1.1.0",
    "@angular-eslint/eslint-plugin-template": "1.1.0",
    "@angular-eslint/schematics": "1.1.0",
    "@angular-eslint/template-parser": "1.1.0",
    "@angular/cli": "11.1.2",
    "@angular/compiler-cli": "11.2.0",
    "@types/jasmine": "3.6.3",
    "@types/jest": "25.2.3",
    "@types/lodash": "4.14.168",
    "@types/node": "12.11.1",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "codelyzer": "6.0.0",
    "eslint": "7.19.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "html-linter": "1.1.1",
    "htmllint-cli": "0.0.7",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "5.0.0",
    "jest": "26.6.3",
    "jest-canvas-mock": "2.3.1",
    "jest-preset-angular": "8.3.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "protractor": "7.0.0",
    "ts-node": "8.3.0",
    "typescript": "4.1.3"
  }
}
